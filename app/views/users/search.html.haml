%h4=link_to 'facebook', user_newsfeed_path

%p * search is case sensitive

= form_tag user_search_path do

  .field
    = label_tag :last_name
    = text_field_tag :last_name, params[:last_name]
    
  .actions
    = submit_tag 'Search', :name => 'search_button'
    
    
    
  - if @users.length > 0
    = render 'form3'
